{
  "name": "arcadia-health",
  "version": "1.0.0",
  "description": "AI-powered healthcare assistant ecosystem for rural and semi-urban India",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "server": "ts-node src/server.ts",
    "start": "node dist/index.js",
    "start:server": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testNamePattern='Property'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "app:server": "cd server && npm run dev",
    "app:client": "cd client && npm run dev",
    "app:seed": "cd server && npm run seed"
  },
  "keywords": [
    "healthcare",
    "ai",
    "rural-health",
    "india",
    "health-education",
    "accessibility"
  ],
  "author": "Arcadia Health Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/uuid": "^9.0.4",
    "cors": "^2.8.6",
    "crypto": "^1.0.1",
    "express": "^5.2.1",
    "fast-check": "^3.13.1",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
